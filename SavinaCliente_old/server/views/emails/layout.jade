- moment.locale('es_CL');

block vars

doctype html
html
  head
    meta(name='viewport', content='width=device-width')
    meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')

    title SAVINA

  body
    div(style='font-family: sans-serif; line-height: 1.48; max-width: 640px; margin: 0 auto; border: 8px solid #ffffff')
      div(style='background-color: #1d79b2; padding: 8px; color: #ffffff;')
        img(
          src='data:image/png;base64,',
          style='display: block; width: auto; max-height: 48px;',
          alt='SAVINA')

      div(style='display: block; background-color: #ffffff')
        h2
          block subtitle

        block body

      div(style='background-color: #1d79b2; color: #ffffff; padding: 8px; font-size: 0.65em;')
        | SAVINA &copy; #{ new Date().getFullYear() }
